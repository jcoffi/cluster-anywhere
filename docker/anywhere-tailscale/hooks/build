#!/bin/bash
docker build --ulimit memlock=-1 --memory-swap=-1 --cache-from=rayproject/ray:py38-cpu --build-arg IMAGETYPE=cpu -f $DOCKERFILE_PATH -t DOCKER_REPO:cpu -t DOCKER_REPO:latest -t DOCKER_REPO:cpu-latest .
docker build --ulimit memlock=-1 --memory-swap=-1 --cache-from=rayproject/ray-ml:py38-gpu --build-arg IMAGETYPE=gpu -f $DOCKERFILE_PATH -t DOCKER_REPO:gpu -t DOCKER_REPO:gpu-latest .